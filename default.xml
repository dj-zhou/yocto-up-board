  <remote fetch="git://git.openembedded.org/" name="oe"/>
  <remote fetch="https://github.com/dj-zhou/" name="up-board"/>
  
  <!-- from yocto -->
  <project remote="yocto" revision="dunfell" name="poky" path="poky"/>
